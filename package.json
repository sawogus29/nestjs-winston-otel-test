{
  "name": "nestjs-winston-app",
  "version": "1.0.0",
  "description": "A NestJS application using Winston as the default logger",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.7",
    "@nestjs/core": "^10.1.7",
    "@nestjs/platform-express": "^10.1.7",
    "nest-winston": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.1.7",
    "@nestjs/testing": "^10.1.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "license": "MIT"
}
